<div class="col-md-8 col-md-offset-3">
  <div class="panel panel-default">
    <div class="panel-heading">
   
<h2>Log in</h2>
<%= button_to "Signin Facebook",user_omniauth_authorize_path(:facebook),class: "btn btn-success" %>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
 
<div class="field">
 <%= f.label :Email %><br/>
    <div class="form-group">
        <%= f.email_field :email, placeholder: "Email", class: "form-control input-lg", label: false, :required =>true, autofocus: true %>
    </div>
  <div class="field">
    <%= f.label :password %><br />
    <div class="form-group">
   <%= f.password_field :password, placeholder: "Password", class: "form-control input-lg", label: false ,:required =>true, autocomplete: "off" %>
   </div>
  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in",class: "btn btn-success" %>
  </div>
<% end %></div></div></div></div></div>

